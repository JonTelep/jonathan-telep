{
    "version": 2,
    "routes": [
      {
        "src": "/script.js",
        "dest": "/script.js"
      },
      {
        "src": "/js/main.js",
        "dest": "/js/main.js"
      },
      {
        "src": "/js/modules/(.*)",
        "dest": "/js/modules/$1"
      },
      {
        "src": "/style.css",
        "dest": "/style.css"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  